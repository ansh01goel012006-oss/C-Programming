#include <stdio.h>

int main() {
    int arr[10], num, i;
    printf("Enter 9 elements:\n");
    for(i=0; i<9; i++)
        scanf("%d", &arr[i]);
    printf("Enter new number to insert at start: ");
    scanf("%d", &num);

    // Shift elements right
    for(i=9; i>0; i--)
        arr[i] = arr[i-1];
    arr[0] = num;

    printf("New array:\n");
    for(i=0; i<10; i++)
        printf("%d ", arr[i]);
    printf("\n");
    return 0;
}
/*
Sample Output:
Enter 9 elements:
11 22 33 44 55 66 77 88 99
Enter new number to insert at start: 100
New array:
100 11 22 33 44 55 66 77 88 99
*/
