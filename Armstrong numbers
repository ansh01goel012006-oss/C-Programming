#include <stdio.h>
#include <math.h>
int main() {
    int n, i, temp, sum, digits, d;
    printf("Enter the range: ");
    scanf("%d", &n);
    for(i = 1; i <= n; i++) {
        temp = i;
        digits = 0;
        sum = 0;
        temp = i;
        while(temp != 0) {
            digits++;
            temp /= 10;
        }
        temp = i;
        while(temp != 0) {
            d = temp % 10;
            sum += pow(d, digits);
            temp /= 10;
        }
        if(sum == i)
            printf("%d ", i);
    }
   
}
