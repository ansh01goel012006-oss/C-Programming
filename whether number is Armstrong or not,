#include <stdio.h>
int main() {
    int n, temp, sum = 0, digit, digits = 0;
    printf("Enter a number: ");
    scanf("%d", &n);
    temp = n;
    while(temp != 0) {
        digits++;
        temp /= 10;
    }
    temp = n;
    while(temp != 0) {
        digit = temp % 10;
        int power = 1;
        for(int i = 0; i < digits; i++)
            power *= digit;
        sum += power;
        temp /= 10;
    }
    if(sum == n)
        printf("Armstrong number");
    else
        printf("Not Armstrong");
    return 0;
}
