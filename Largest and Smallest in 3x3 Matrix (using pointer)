#include <stdio.h>
#define SIZE 3

int main() {
    int mat[SIZE][SIZE] = { {1,9,3}, {4,2,8}, {6,5,7} };
    int *p = (int *)mat;
    int max=*p, min=*p;

    for(int i=0; i<SIZE*SIZE; i++) {
        if(*(p+i) > max) max = *(p+i);
        if(*(p+i) < min) min = *(p+i);
    }

    printf("Largest: %d
", max);
    printf("Smallest: %d
", min);

    return 0;
}