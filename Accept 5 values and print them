#include <stdio.h>

int main() {
    int arr[5];
    int i;
    printf("Enter 5 values:\n");
    for(i=0; i<5; i++){
        scanf("%d", &arr[i]);
    }
    printf("You entered:\n");
    for(i=0; i<5; i++){
        printf("%d ", arr[i]);
    }
    printf("\n");
    return 0;
}

/*
Sample Output:
Enter 5 values:
11 22 33 44 55
You entered:
11 22 33 44 55
*/
