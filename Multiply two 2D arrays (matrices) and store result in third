#include <stdio.h>

int main() {
    int a[2][2], b[2][2], mul[2][2] = {{0,0},{0,0}};
    int i, j, k;
    printf("Enter elements of 1st 2x2 matrix:\n");
    for(i=0; i<2; i++)
        for(j=0; j<2; j++)
            scanf("%d", &a[i][j]);
    printf("Enter elements of 2nd 2x2 matrix:\n");
    for(i=0; i<2; i++)
        for(j=0; j<2; j++)
            scanf("%d", &b[i][j]);
    for(i=0; i<2; i++)
        for(j=0; j<2; j++)
            for(k=0; k<2; k++)
                mul[i][j] += a[i][k] * b[k][j];

    printf("Multiplication matrix:\n");
    for(i=0; i<2; i++) {
        for(j=0; j<2; j++)
            printf("%d ", mul[i][j]);
        printf("\n");
    }
  
}
