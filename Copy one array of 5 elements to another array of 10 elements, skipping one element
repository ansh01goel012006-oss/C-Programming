#include <stdio.h>

int main() {
    int arr1[5], arr2[10];
    int i, j=0;
    printf("Enter 5 values for arr1:\n");
    for(i=0; i<5; i++)
        scanf("%d", &arr1[i]);
    for(i=0; i<10; i++)
        arr2[i] = 0;

    for(i=0; i<5; i++) {
        if(i==2)
            continue; // skip 3rd element
        arr2[j++] = arr1[i];
    }
    printf("Second array after copying (skipping one element):\n");
    for(i=0; i<10; i++)
        printf("%d ", arr2[i]);
    printf("\n");
   
}

