#include <stdio.h>
int main() {
    int n, rev = 0, digit;
    printf("Enter a number: ");
    scanf("%d", &n);
    while(n > 0) {
        rev = rev * 10 + n % 10;
        n = n / 10;
    }
    while(rev > 0) {
        digit = rev % 10;
        printf("%d ", digit);
        rev = rev / 10;
    }

}
