#include <stdio.h>
#include <math.h>
int main() {
    int i, t, sum, digits, x;
    for(i = 1; i <= 1000; i++) {
        t = i; digits = 0;
        while(t != 0) {
            digits++;
            t = t / 10;
        }
        t = i; sum = 0;
        while(t != 0) {
            x = t % 10;
            sum += pow(x, digits);
            t = t / 10;
        }
        if(sum == i)
            printf("%d ", i);
    }
   
}
