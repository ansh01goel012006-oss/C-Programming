#include <stdio.h>

int main() {
    int a[2][2], b[2][2], result[2][2] = {0};
    int i, j, k;
    printf("Enter first 2x2 matrix:\n");
    for(i=0; i<2; i++)
        for(j=0; j<2; j++)
            scanf("%d", &a[i][j]);
    printf("Enter second 2x2 matrix:\n");
    for(i=0; i<2; i++)
        for(j=0; j<2; j++)
            scanf("%d", &b[i][j]);
    for(i=0; i<2; i++)
        for(j=0; j<2; j++)
            for(k=0; k<2; k++)
                result[i][j] += a[i][k] * b[k][j];
    printf("Product matrix:\n");
    for(i=0; i<2; i++) {
        for(j=0; j<2; j++)
            printf("%d ", result[i][j]);
        printf("\n");
    }
    return 0;
}
/*
Sample Output:
Enter first 2x2 matrix:
1 2 3 4
Enter second 2x2 matrix:
5 6 7 8
Product matrix:
19 22
43 50
*/
