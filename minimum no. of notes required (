#include <stdio.h>

int main() {
    int notes[] = {500, 200, 100, 50, 20, 10, 5, 2, 1};
    int amount, i;
    printf("Enter amount: ");
    scanf("%d", &amount);
    for(i=0; i<9; i++) {
        if(amount >= notes[i]) {
            printf("%d notes of %d\n", amount/notes[i], notes[i]);
            amount = amount % notes[i];
        }
    }
    return 0;
}
/*
Sample Output:
Enter amount: 1256
2 notes of 500
1 notes of 200
1 notes of 50
1 notes of 5
1 notes of 1
*/
