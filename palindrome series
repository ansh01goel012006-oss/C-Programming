#include <stdio.h>
int main() {
    int n, i, num, rev, temp;
    printf("Enter the range: ");
    scanf("%d", &n);
    for(i = 1; i <= n; i++) {
        num = i;
        rev = 0;
        temp = num;
        while(temp > 0) {
            rev = rev*10 + temp%10;
            temp /= 10;
        }
        if(num == rev)
            printf("%d ", num);
    }
   
}
