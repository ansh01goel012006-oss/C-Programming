#include <stdio.h>
int main() {
    int n, i, num, even = 0, odd = 0;
    printf("Enter how many numbers: ");
    scanf("%d", &n);
    for(i = 1; i <= n; i++) {
        printf("Enter number %d: ", i);
        scanf("%d", &num);
        if(num % 2 == 0)
            even++;
        else
            odd++;
    }
    printf("Even: %d\nOdd: %d", even, odd);

}
